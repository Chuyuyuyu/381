  function add_text(id){
    var flag = 0;
    var o = document.getElementById('comment_block');
    if (o.value == 'Âú®ËøôÈáåËæìÂÖ•ÊÇ®ÁöÑËØÑËÆ∫...') o.value = '';
    for (var i = 0;i <=9;i++){
        var idx = o.value.indexOf(lan[i]);
        if ( idx != -1){
            flag = 1;
            break;
        }
    }
    if ( flag == 1)
        alert("‰Ω†Â∑≤ÁªèÈÄâÊã©‰∫Ü‰∏ÄÊù°Âø´Êç∑ÂèëË®ÄÂì¶ÔºåÊØèÊ¨°ÂõûÂ§çÂè™ËÉΩÂåÖÂê´‰∏ÄÊù°Âø´Êç∑‰ø°ÊÅØÔºÅ");
    else{
        o.value = o.value+lan[id];
    }
    document.getElementById('dialog_txt').style.display="none";
    o.focus();
  }
  function show_text(){
	document.writeln('<ul>');
    for (var i = 0;i <=9;i++)
        document.writeln('<li><a href="javascript:add_text('+i+');">'+lan[i]+'</a></li>');
	document.writeln('</ul>');
  }
  

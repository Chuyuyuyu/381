  function submit_reply(f){
    f.reply.value=f.reply_show.value;
    if(f.reply.value.length < 3){
        alert('ÂõûÂ§çÂÜÖÂÆπ‰∏çÂæóÂ∞è‰∫é5‰∏™Â≠óÁ¨¶ÔºÅÂ§öÂÜôÂá†‰∏™Â≠óÂêßÔΩûÔΩû');
        f.reply_show.focus();
        return false;
    }
	var new1 = f.reply.value;
	new1 = new1.replace(/\[FA:[0-9][0-9]\]/g,"");	
    if(new1.length < 3){
        alert('ÂõûÂ§çÂÜÖÂÆπ‰∏çÂæóÂ∞è‰∫é3‰∏™Â≠óÁ¨¶!ÂõûÂ§ç‰∏≠‰∏çËÉΩÂè™ÊúâË°®ÊÉÖÔºÅ');
        f.reply_show.focus();
        return false;
    }	
    var mycid = f.cid.value;
	var str= f.reply.value;
    str =str.replace(/script/ig,"");
    str =str.replace(/java/ig,"");
    str =str.replace(/</ig,"");
    str =str.replace(/>/ig,"");	
    var show_comment = str.replace(/\[FA:([0-9][0-9])\]/g,"<img src='/p2images/$1.gif'>");	
    document.getElementById('reply_add_to_this_'+mycid).innerHTML='<div class="hf1"><div class="hf_le"><div class="hf_wj"><b>Êàë</b><span>4399Áé©ÂÆ∂</span><em>ÂõûÂ§ç‰∫é '+now+'</em></div><p>'+show_comment+'</p></div><div class="hf_ri"><?=$cnt?></div><div class="clear"></div></div>';	
    f.reply_show.value='';
    return true;
  } 
